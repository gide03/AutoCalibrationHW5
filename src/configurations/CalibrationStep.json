{
  "Environment": {
    "Connectivity": {
      "TestBench": {
        "UseGeny": true,
        "SerialPort": "COM1",
        "BaudRate": 9600
      },
      "DlmsClient": {
        "SerialPort": "COM15",
        "BaudRate": 19200,
        "InterOctetTimeout": 0.1,
        "InactivityTimeout": 10,
        "LoginRetry": 1,
        "MeterAddress": 16,
        "ClientId": 115
      },
      "FrequencyCounter": {
        "InstrumentPath": "USB0::0x14EB::0x0090::389645::INSTR",
        "VendorList": {
          "PENDULUM": true,
          "KEYSIGHT": false
        }
      }
    }
  },

  "step 1": {
    "processName": "Apply power to meter",
    "parameters": {
      "Voltage": 230,
      "Current": 40,
      "Phase": 60,
      "Frequency": 50
    },
    "isEnable": true
  },

  "step 2": {
    "processName": "Power up delay",
    "parameters": {
      "DelayTime": 12
    },
    "isEnable": true
  },

  "step 3": {
    "processName": "Check is calibration data in default",
    "parameters": {
      "Set default value": false,
      "Default meter setup": {
        "MeterForm": 132,
        "MeterClass": 5,
        "FrequencySelection": 0,
        "MeterType": 4, 
        "MeterVoltageType": 0,
        "MeterPowerSupplyType": 1,
        "ServicesenseMode": 0,
        "AnticreepConstant" : 748,
        "AnticreepVoltage" : 1400,
        "AnticreepTimer" : 92160000,
        "Meter_SKU": 168296089,
        "LoadControlSets" : 1,
        "Main_switch_bistable_open_halfms_pulse_width": 80,
        "Main_switch_bistable_close_halfms_pulse_width" : 80,
        "Main_switch_pre_cross_halfms": 255,
        "Aux_switch_bistable_open_halfms_pulse_width" : 80,
        "Aux_switch_bistable_close_halfms_pulse_width" : 80,
        "Aux_switch_pre_cross_halfms" : 255,
        "LCR1_switch_bistable_open_halfms_pulse_width" : 80,
        "LCR1_switch_bistable_close_halfms_pulse_width" : 80,
        "LCR1_switch_pre_cross_halfms" : 255,
        "LCR2_switch_bistable_open_halfms_pulse_width" : 80,
        "LCR2_switch_bistable_close_halfms_pulse_width" : 80,
        "LCR2_switch_pre_cross_halfms" : 255,

        "PhShiftTransferFunctionControl": 3,
        "Scalar_Config_For_FullAmorphous" : 14,
        "MA_FilterCoeff_0" : 15793,
        "MA_FilterCoeff_1" : -6665,
        "MA_FilterCoeff_2" : -9052,
        "MA_FilterCoeff_3" : 0,
        "MA_FilterCoeff_4" : 0,
        "MA_FilterCoeff_5" : 0,
        "MA_FilterCoeff_6" : 0,
        "MA_FilterCoeff_7" : 0,
        "MA_FilterCoeff_8" : 0,
        "MA_FilterCoeff_9" : 0,
        "MA_FilterCoeff_10" : 0,
        "MA_FilterCoeff_11" : 0,
        "MA_FilterCoeff_12" : 0,
        "MA_FilterCoeff_13" : 0,
        "MA_FilterCoeff_14" : 0,
        "MA_FilterCoeff_15" : 0,
        "AR_FilterCoeff_0" : 7938,
        "AR_FilterCoeff_1" : 8428,
        "AR_FilterCoeff_2" : 0
      },
      "Default calibration data": {
        "PhDirectionA": 1,
        "PhDirectionB": 1,
        "PhDirectionC": 1,
        "PhDirectionN": 1,
        "PhaseDelayA": 0,
        "PhaseDelayB": 0,
        "PhaseDelayC": 0,
        "PhaseDelayN": 0
      }
    },
    "isEnable": true
  },

  "step 4": {
    "processName": "Read and save temperature information",
    "parameters": {
      "filename": "temperature.txt",
      "isSaveToFile": true
    },
    "isEnable": true
  },

  "step 5": {
    "processName": "Miscelaneous configurations",
    "parameters": {
      "NoPulseParameter": 0
    },
    "isEnable": true
  },

  "step 6": {
    "processName": "Calibrate",
    "parameters": {
      "NumOfCycles": 90,
      "RtcMode": 0,
      "TurnOffMeterOnFailed": true
    },
    "isEnable": true
  },

  "step 7": {
    "processName": "Read RTC",
    "parameters": {
      "NumOfSampleTaken": 1,
      "MinFreqAccepted": 4,
      "MaxFreqAccepted": 4.01,
      "EnableMeasurementFilter": false
    },
    "isEnable": true
  },

  "step 8": {
    "processName": "Delay for test to complete",
    "parameters": {
      "DelayTime": 2
    },
    "isEnable": true
  },

  "step 9": {
    "processName": "Read instant voltage and current from Cal Mode reading, temperature and the standard",
    "parameters": {
      "InstantRegisters": [
        "InstantVoltagePhase1",
        "InstantVoltagePhase2",
        "InstantVoltagePhase3",
        "InstantCurrentPhase1",
        "InstantCurrentPhase2",
        "InstantCurrentPhase3"
      ]
    },
    "isEnable": true
  },

  "step 10": {
    "processName": "Calculate gain and write to calibration data",
    "parameters": {
      "summarize": false
    },
    "isEnable": true
  }
}
