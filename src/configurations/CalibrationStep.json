{
  "Environment": {
    "Connectivity": {
      "TestBench": {
        "UseGeny": false,
        "SerialPort": "COM0",
        "BaudRate": 9600
      },
      "DlmsClient": {
        "SerialPort": "COM4",
        "BaudRate": 19200,
        "InterOctetTimeout": 0.1,
        "InactivityTimeout": 10,
        "LoginRetry": 1,
        "MeterAddress": 16,
        "ClientId": 115
      }
    }
  },

  "step 1": {
    "processName": "Apply power to meter",
    "parameters": {
      "Voltage": 230,
      "Current": 30,
      "Phase": 60,
      "Frequency": 50
    },
    "isEnable": true
  },

  "step 2": {
    "processName": "Power up delay",
    "parameters": {
      "DelayTime": 2
    },
    "isEnable": true
  },

  "step 3": {
    "processName": "Check is calibration data in default",
    "parameters": {
      "Set default value": false,
      "Default meter setup": {
        "MeterForm": 133,
        "MeterClass": 5,
        "FrequencySelection": 0,
        "MeterType": 1,
        "MeterVoltageType": 0,
        "MeterPowerSupplyType": 1,
        "ServicesenseMode": 0,
        "AnticreepConstant": 793
      }
    },
    "isEnable": true
  },

  "step 4": {
    "processName": "Read and save temperature information",
    "parameters": {
      "filename": "temperature.txt",
      "isSaveToFile": false
    },
    "isEnable": true
  },

  "step 5": {
    "processName": "Program LED pulse output",
    "parameters": {
      "NoPulseParameter": 0
    },
    "isEnable": true
  },

  "step 6": {
    "processName": "Calibrating",
    "parameters": {
      "NumOfCycles": 90,
      "RtcMode": 0
    },
    "isEnable": true
  },

  "step 7": {
    "processName": "Read RTC",
    "parameters": {
      "NumOfSampleTaken": 1,
      "MinFreqAccepted": 4,
      "MaxFreqAccepted": 4.01,
      "EnableMeasurementFilter": false
    },
    "isEnable": true
  },

  "step 8": {
    "processName": "Delay for test to complete",
    "parameters": {
      "DelayTime": 2
    },
    "isEnable": true
  },

  "step 9": {
    "processName": "Read instant voltage and current from Cal Mode reading, temperature and the standard",
    "parameters": {
      "InstantRegisters": [
        "InstantVoltagePhase1",
        "InstantVoltagePhase2",
        "InstantVoltagePhase3",
        "InstantCurrentPhase1",
        "InstantCurrentPhase2",
        "InstantCurrentPhase3"
      ]
    },
    "isEnable": true
  },

  "step 10": {
    "processName": "Calculate gain and write to calibration data",
    "parameters": {
      "summarize": false
    },
    "isEnable": true
  }
}
